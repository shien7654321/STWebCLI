        "@typescript-eslint/eslint-plugin": "^8.7.0",
        "@typescript-eslint/parser": "^8.7.0",
        "@vercel/ncc": "^0.38.2",
        "cross-env": "^7.0.3",
        "eslint": "9.11.1",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.30.0",
        "nodemon": "^3.1.7",
        "prettier": "^3.3.3",
        "rimraf": "^6.0.1",
        "ts-node": "^10.9.2",
        "tslib": "^2.7.0",
        "typescript": "^5.6.2"
