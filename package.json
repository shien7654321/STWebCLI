{
  "name": "st-web-cli-template",
  "version": "1.0.0",
  "main": "src/bin/index.js",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shien7654321/STWebCLI.git#tpl-node-js"
  },
  "scripts": {
    "build": "rimraf ./dist && javascript-obfuscator src --output dist/src && cp -RP ./node_modules ./dist/node_modules && cp ./package.json ./dist/package.json",
    "build-minify": "rimraf ./dist && ncc build -m -s --no-source-map-register src/bin/index.js -o dist",
    "dev": "cross-env NODE_ENV=development nodemon",
    "lint": "eslint --fix",
    "prettier": "prettier --write \"src\""
  },
  "engines": {
    "node": ">= 20.0.0",
    "npm": ">= 10.0.0"
  },
  "dependencies": {
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.22.0",
    "@vercel/ncc": "^0.38.3",
    "cross-env": "^7.0.3",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "javascript-obfuscator": "^4.1.1",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1"
  },
  "imports": {
    "#*": "./src/*"
  }
}
