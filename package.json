{
  "name": "st-web-cli-template",
  "version": "1.0.0",
  "main": "src/main.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shien7654321/STWebCLI.git#tpl-vue3-webpack-ssr"
  },
  "scripts": {
    "build": "yarn build:client && yarn build:server && yarn build:node",
    "build:client": "TS_NODE_PROJECT=\"build/webpack.tsconfig.json\" webpack --progress --config build/webpack.prod.ts",
    "build:server": "TS_NODE_PROJECT=\"build/webpack.tsconfig.json\" webpack --progress --config build/webpack.server.ts",
    "build:node": "TS_NODE_PROJECT=\"build/webpack.tsconfig.json\" webpack --progress --config build/webpack.node.ts",
    "ssr": "nodemon dist/node/server.js --config nodemon.json",
    "dev": "TS_NODE_PROJECT=\"build/webpack.tsconfig.json\" webpack serve --progress --config build/webpack.dev.ts",
    "dev:ssr": "nodemon server/devSSR.ts --config nodemon.json",
    "lint": "eslint --fix \"src/**/*.{vue,js,ts,jsx,tsx}\"",
    "lint-style": "stylelint --fix \"src/**/*.{html,vue,css,less,sass,scss,styl,stylus,jsx,tsx}\"",
    "prettier": "prettier --write \"src\""
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 6.0.0"
  },
  "dependencies": {
    "@vue/server-renderer": "^3.5.12",
    "chalk": "4.1.2",
    "express": "4.21.0",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/register": "^7.25.9",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "@vue/babel-plugin-jsx": "^1.2.5",
    "@vue/compiler-sfc": "^3.5.12",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "compression-webpack-plugin": "^11.1.0",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-vue": "^9.30.0",
    "html-webpack-plugin": "^5.6.3",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "memory-fs": "^0.5.0",
    "mini-css-extract-plugin": "^2.9.2",
    "nodemon": "^3.1.7",
    "null-loader": "^4.0.1",
    "postcss": "^8.4.47",
    "postcss-html": "^1.7.0",
    "postcss-less": "^6.0.0",
    "postcss-loader": "^8.1.1",
    "postcss-sass": "^0.5.0",
    "postcss-scss": "^4.0.9",
    "postcss-styl": "^0.12.3",
    "prettier": "^3.3.3",
    "process": "^0.11.10",
    "sass": "^1.80.6",
    "sass-loader": "^16.0.3",
    "stylelint": "^16.10.0",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.1",
    "stylus": "^0.64.0",
    "stylus-loader": "^8.1.1",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3",
    "uglify-js": "^3.19.3",
    "vue-loader": "^17.4.2",
    "vue-style-loader": "^4.1.3",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^7.4.2",
    "webpack-dev-server": "^5.1.0",
    "webpack-hot-middleware": "^2.26.1",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-merge": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  }
}
